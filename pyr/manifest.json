{
    "manifest_version": 2,
    "name": "PYR Http",
    "version": "1.0",
  
    "description": "A helper tool to incercept http(s) request",
  
    "icons": {
      "96": "icons/st-96.png"
    },

    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],

    "background": {
      "scripts": ["http.js"]
    }
}